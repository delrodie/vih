{% extends 'layout.html.twig' %}

{% block body %}
    <div class="pages">
        <div data-page="projects" class="page no-toolbar no-navbar">
            <div class="page-content">

                <div class="navbarpages">
                    <div class="nav_left_logo"><a href="#" data-panel="left" class="open-panel"><img src="{{ absolute_url(asset('ressources/images/logo-02.png')) }}" alt="" title="" /></a></div>
                    <div class="nav_right_button">
                        <a href="#" data-panel="left" class="open-panel"><img src="{{ absolute_url(asset('ressources/images/icons/white/menu.png')) }}" alt="" title="" /></a>
                        <a href="{{ path('admin_gestionnaire_index') }}"><img src="{{ asset('ressources/images/icons/white/back.png') }}" alt="" title="" /></a>
                    </div>
                </div>
                <div id="pages_maincontent">

                    <h2 class="page_title">Enregistrement du gestionnaire</h2>

                    <div class="page_content">

                        <div class="contactform" id="contactForm">
                            {{ form_start(form) }}


                            <label>Utilisateur:</label>
                            {{ form_widget(form.user) }}

                            <label>Zone:</label>
                            {{ form_widget(form.zone) }}

                            <label>Nom:</label>
                            {{ form_widget(form.nom) }}

                            <label>Prenoms:</label>
                            {{ form_widget(form.prenom) }}

                            <label>Contact:</label>
                            {{ form_widget(form.contact) }}

                            <input type="submit" name="submit" class="form_submit"   value="Envoyer" />
                            {{ form_end(form) }}
                        </div>



                    </div>

                </div>


            </div>
        </div>
    </div>
{% endblock %}

